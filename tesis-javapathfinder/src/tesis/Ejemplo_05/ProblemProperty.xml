<?xml version="1.0" ?>
<ProblemDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ProblemDefinition.xsd">
	<TypeStateProperty class="tesis.Ejemplo.Canal">
	    <states>
	        <state label="0" start="true" />
	        <state label="1" />
	        <state label="999" final="true" />
	    </states>
	    <transitions>
	        <transition from="0" to="1" labelEvent="C" />
	        <transition from="1" to="0" labelEvent="O" />
	        <transition from="1" to="1" labelEvent="C" />
	        <transition from="1" to="999" labelEvent="W" />
	    </transitions>
	</TypeStateProperty>
	<GlobalProperty>
	    <states>
	        <state label="0" start="true" />
	        <state label="1" />
	        <state label="999" final="true" />
	    </states>
	    <transitions>
	        <transition from="0" to="1" labelEvent="C" />
	        <transition from="1" to="0" labelEvent="O" />
	        <transition from="1" to="1" labelEvent="C" />
	        <transition from="1" to="999" labelEvent="W" />
	    </transitions>
	</GlobalProperty>
</ProblemDefinition>