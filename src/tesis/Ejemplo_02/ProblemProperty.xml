<?xml version="1.0" ?>
<ProblemDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ProblemDefinition.xsd">
	<GlobalProperty>
	    <states>
	        <state label="1" start="true" />
	        <state label="2" />
	        <state label="3" />
	        <state label="ERROR" final="true" />
	    </states>
	    <transitions>
	        <transition from="1" to="2" labelEvent="A" />
	        <transition from="2" to="3" labelEvent="C" />
	        <transition from="3" to="ERROR" labelEvent="B" />
	    </transitions>
	</GlobalProperty>
</ProblemDefinition>